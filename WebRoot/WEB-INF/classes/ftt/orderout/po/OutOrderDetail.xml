<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ftt.orderout.domain.OutOrderDetail" table="OUT_ORDER_DETAIL">
        <id name="id" type="long" column="ID">
            <generator class="native" />
        </id>
        <property name="goodsNo" type="string" column="GOODS_NO"/>
        <property name="brandName" type="string" column="BRAND_NAME"/>
        <property name="colorName" type="string" column="COLOR_NAME"/>
        <property name="size" type="string" column="SIZE"/>
        <property name="number" type="int" column="NUMBER"/>
	
		<many-to-one name="outOrder" column="OUTORDER_ID" 
			class="ftt.orderout.domain.OutOrder" 
			lazy="false"/>        
    </class>
</hibernate-mapping>
